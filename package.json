    "oidc-client-ts": "^3.0.1",
    "pinia": "^2.1.7",
    "vite-plugin-vuetify": "^2.0.3",
    "vue": "^3.4.29",
    "vue-router": "^4.3.3",
    "vuetify": "^3.6.10"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.8.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.0",
